    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.12.1",
    "nodemon": "^1.18.9"
  },
  "dependencies": {
    "cookie-parser": "^1.4.3",
    "dateformat": "^3.0.3",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "winston": "^3.1.0"
  }
}
