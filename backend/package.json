{
  "name": "bcpa-website-backend",
  "description": "BCPA Website Backend",
  "author": "Max Fuller",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.js",
  "engines": {
    "node": "9.4.0"
  },
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node"
  },
